{
  "$schema": "https://biomejs.dev/schemas/1.0.0/schema.json",
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "all": true
    }
  },
  "formatter": {
    "enabled": true,
    "formatWithErrors": false,
    "indentSize": 2,
    "lineWidth": 80,
    "indentStyle": "tab"
  },
  "javascript":{
    "formatter":{"semicolons":"asNeeded","enabled": true,"quoteStyle":"single"}
  }
}
